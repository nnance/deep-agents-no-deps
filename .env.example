# Example .env file for testing the environment management library
# This file demonstrates various formatting options

# Basic key-value pairs
NODE_ENV=development
PORT=3000

# Database configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=my_app_db
DB_USER=admin

# Quoted values
DB_PASSWORD="secure_password_123"
API_URL="https://api.example.com"

# Values with single quotes
APP_NAME='Deep Agents System'
LOG_LEVEL='debug'

# Empty values
OPTIONAL_FEATURE=
EMPTY_CONFIG=""

# Values with special characters
JWT_SECRET="abc123!@#$%^&*()_+-={}[]|:;'<>?,./`~"
WEBHOOK_URL="https://example.com/webhook?token=abc123&format=json"

# Boolean-like values
DEBUG=true
ENABLE_CACHE=false
MAINTENANCE_MODE=0

# Comments are ignored
# This is a comment line

# Numeric values (stored as strings)
MAX_CONNECTIONS=100
TIMEOUT_MS=5000
RETRY_COUNT=3
